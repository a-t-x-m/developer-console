{
  "name": "@atxm/developer-console",
  "version": "0.2.2",
  "description": "Wrapper to limit console output to Atom in Developer Mode",
  "license": "MIT OR GPL-2.0",
  "scripts": {
    "build": "rimraf tsconfig.tsbuildinfo && tsc --pretty --project ./",
    "dev": "npm run start",
    "lint": "tslint --project ./",
    "start": "tsc --watch --pretty --project ./",
    "test": "npm run lint"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-t-x-m/developer-console.git"
  },
  "keywords": [
    "atom",
    "atom package"
  ],
  "dependencies": {
    "atom-read-manifest": "^0.5.2"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/node": "^14.11.8",
    "husky": "^4.3.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
